{
  "$schema": "node_modules/@lerna-lite/cli/schemas/lerna-schema.json",
  "version": "0.8.0",
  "useWorkspaces": true,
  "command": {
    "version": {
      "allowBranch": [
        "main"
      ],
      "yes": true,
      "conventionalCommits": true,
      "message": "chore(release): Publish %s [skip ci]"
    }
  },
  "ignoreChanges": [
    "**/CHANGELOG.md"
  ],
  "yes": true,
  "syncWorkspaceLock": true
}
